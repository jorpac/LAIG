<lxs>
    <scene root="root" axis_length="5"/>

    <views default="defaultCamera">
        <perspective id="defaultCamera" near="0.1" far="500" angle="45">
            <from x="30" y="15" z="30" />
            <to x="0" y="-2" z="0" />
        </perspective>
    </views>

    <globals>
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
        <background r="0.0" g="0.0" b="0.0" a="1.0" />
    </globals>

    <lights>
        <omni id="demoLight" enabled="1">
            <location x="9.8" y="11.9" z="10" w="1.0" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
        </omni>


        <spot id="spotlight" enabled="1" angle="45" exponent="1">

            <!-- atencao, "target" e' diferente de "direction" -->
            <location x="25" y="0" z="0" w="1" />
            <target x="20" y="0" z="0" />
            <ambient r="1.0" g="1.0" b="1.0" a="1" />
            <diffuse r="0" g="0" b="0" a="0" />
            <specular r="1.0" g="1.0" b="1.0" a="1" />
            <attenuation constant="0" linear="0" quadratic="0" />
        </spot>
        
        <spot id="spotlight2" enabled="1" angle="-45" exponent="1">

            <!-- atencao, "target" e' diferente de "direction" -->
            <location x="10" y="5" z="5" w="1" />
            <target x="0" y="0" z="0" />
            <ambient r="1.0" g="1.0" b="1.0" a="1" />
            <diffuse r="0" g="0" b="0" a="0" />
            <specular r="0" g="0" b="0" a="1" />
            <attenuation constant="1" linear="0" quadratic="0" />
        </spot>
    </lights>

    <textures>
        <texture id="demoTexture" file="scenes/images/dark_glass.jpg" />
        <texture id="treeTrunkImg" file="scenes/images/trunk-image.jpg" />
        <texture id="treeTopText" file="scenes/images/leaves.jpg" />
        <texture id="roofTopText" file="scenes/images/metalTex.jpg" />
        <texture id="floorText" file="scenes/images/grass.png" />
        <texture id="houseWall" file="scenes/images/woodenWallTex.png" />
        <texture id="car_front" file="scenes/images/front_car.jpg" />
        <texture id="car_back" file="scenes/images/back_car.jpg" />
        <texture id="car_side" file="scenes/images/side_car.jpg" />
        <texture id="car_roof" file="scenes/images/roof_car.jpg" />
        <texture id="car_roof1" file="scenes/images/roof1_car.jpg" />
        <texture id="back_window" file="scenes/images/back_window.jpg" />
        <texture id="asphalt" file="scenes/images/asphalt.jpg" />
        <texture id="beton" file="scenes/images/beton.jpg" />
        <texture id="bancada" file="scenes/images/crowd.jpg" />
    </textures>

    <materials>
        <material id="demoMaterial" shininess="1">
            <emission r="0.0" g="0.0" b="0.0" a="1.0" />
            <ambient r="1.0" g="0.0" b="0.0" a="1.0" />
            <diffuse r="0.6" g="0.0" b="0.0" a="1.0" />
            <specular r="0.6" g="0.0" b="0.0" a="1.0" />
        </material>
    </materials>

    <transformations>
        <transformation id="demoTransform">
            <scale x="0.5" y="0.5" z="0.5" />
            <rotate axis="z" angle="-45" />
            <rotate axis="x" angle="-45" />
        </transformation>

    </transformations>

    <primitives>
        <primitive id="rectangle">
            <rectangle x1="-0.5" y1="-1" x2="0.5" y2="1" />
        </primitive>
       
        <primitive id="cylinder">
            <cylinder base="0.5" top="0.5" height="2" slices="50" stacks="50"/>
        </primitive>
         <primitive id="acylinder">
            <cylinder base="0.5" top="2" height="2" slices="50" stacks="50"/>
        </primitive>
        <primitive id="track">
            <torus outer="20" inner="3" slices="30" loops="100"/>
        </primitive>
        <primitive id="out_floor">
            <torus outer="24" inner="1" slices="30" loops="100"/>
        </primitive>
          <primitive id="floor_cylinder">
            <cylinder base="17" top="0" height="0" slices="50" stacks="50"/>
        </primitive>
        
         <primitive id="stadium">
            <cylinder base="25" top="30" height="10" slices="50" stacks="50"/>
        </primitive>
        <primitive id = "sphere">
            <sphere radius="1" slices = "40" stacks="40"/>
        </primitive>
        <primitive id = "triangle">
            <triangle x1="-1" y1="0" z1="0" x2="1" y2="0" z2="0" x3="0" y3="1.415" z3="0"/>
        </primitive>
         <primitive id = "rectriangle">
            <triangle x1="0" y1="0" z1="0" x2="0" y2="0.9" z2="0" x3="0.5" y3="0" z3="0"/>
        </primitive>
        <primitive id = "torus">
            <torus outer="1" inner="0.5" slices="30" loops="30"/>
        </primitive>
    </primitives>

    <components>
        <component id="root">
            <transformation>
                <!-- <translate x="0" y="0" z="1" />
                <scale x="1.5" y="1" z="1" /> -->
            </transformation>
            <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="none"/>
            <children>
                <componentref id="forest"/>
                <componentref id="house"/>
                <!-- <componentref id = "back_floor" /> -->
                <componentref id="sculp"/>
                <componentref id = "farol"/>
                <componentref id = "stand"/>
                <componentref id = "stadiummm"/>
            </children>
        </component>

        <component id="stadiummm">
            <transformation>
            </transformation>
            <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="none"/>
            <children>
                <componentref id = "stadiumm"/>    
                <componentref id="pista"/>
        </children>
        </component>
        
            <component id="stadiumm">
            <transformation>
                <scale x="1.3" y="1" z="1" />
                <rotate axis="x" angle="-90"/>
            </transformation>
            <materials>
