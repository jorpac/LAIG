<lxs>
    <scene root="root" axis_length="5"/>

    <views default="defaultCamera">
        <perspective id="defaultCamera" near="0.1" far="500" angle="45">
            <from x="30" y="15" z="30" />
            <to x="0" y="-2" z="0" />
        </perspective>
    </views>

    <ambient>
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
        <background r="0.0" g="0.0" b="0.0" a="1.0" />
    </ambient>

    <lights>
        <omni id="demoLight" enabled="1">
            <location x="9.8" y="11.9" z="10" w="1.0" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
        </omni>
    </lights>

    <textures>
        <texture id="demoTexture" file="scenes/images/vidral.jpg" />
    </textures>

    <materials>
        <material id="demoMaterial" shininess="10">
            <emission r="0.0" g="0.0" b="0.0" a="1.0" />
            <ambient r="1.0" g="0.0" b="0.0" a="1.0" />
            <diffuse r="0.6" g="0.0" b="0.0" a="1.0" />
            <specular r="0.6" g="0.0" b="0.0" a="1.0" />
        </material>
    </materials>

    <transformations>
        <transformation id="demoTransform">
            <scale x="0.5" y="0.5" z="0.5" />
            <rotate axis="z" angle="-45" />
            <rotate axis="x" angle="-45" />
        </transformation>

    </transformations>

    <primitives>
        <primitive id="rectangle">
            <rectangle x1="-0.5" y1="-1" x2="0.5" y2="1" />
        </primitive>
        <primitive id="cylinder">
            <cylinder base="0.5" top="0.5" height="2" slices="50" stacks="50"/>
        </primitive>
        <primitive id = "sphere">
            <sphere radius="1" slices = "40" stacks="40"/>
        </primitive>
        <primitive id = "triangle">
            <triangle x1="-1" y1="0" z1="0" x2="1" y2="0" z2="0" x3="0" y3="1.415" z3="0"/>
        </primitive>
        <primitive id = "torus">
            <torus outradius="2" inradius="1" slices="300" loops="300"/>
        </primitive>
    </primitives>

    <components>
        <component id="root">
            <transformation>
                <!-- <translate x="0" y="0" z="1" />
                <scale x="1.5" y="1" z="1" /> -->
            </transformation>
            <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="demoTexture"/>
            <children>
                <componentref id="forest"/>
                <componentref id="house"/>
                <componentref id = "back_floor" />
                <componentref id="sculp"/>
                <!-- <componentref id="retanguloTeste"/>  -->
                <!-- <primitiveref id="torus" /> -->
                <!--<primitiveref id="demoRectangle" />
                <primitiveref id="newCylinder"/>
                <primitiveref id = "sphere"/>
                <primitiveref id = "triangle"/>-->
            </children>
        </component>

        <component id="treetrunk">
            <transformation>
                <rotate axis="x" angle="-90" />
                <scale x = "0.2" y = "0.2" z = "0.5" />
            </transformation>
             <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="demoTexture"/>
            <children>
                <primitiveref id = "cylinder" />
            </children>
        </component>
 
        <component id = "treetop">
            <transformation>
                <translate x = "0" y = "1" z = "0" />
                <rotate axis="x" angle="-90" /> 
                <scale x = "0.4" y = "0.4" z = "0.5" />
            </transformation>
             <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="demoTexture"/>
            <children>
                <primitiveref id = "sphere" />
            </children>
        </component>

        <component id = "tree_0">
            <transformation>
                <translate x = "0" y = "0" z = "2" />
            </transformation>
             <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="demoTexture"/>
            <children>
                <componentref id="treetop"/>
                <componentref id="treetrunk"/>
            </children>
        </component>
        <component id = "tree_1">
            <transformation>
                <translate x = "2" y = "0" z = "5" />
            </transformation>
             <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="demoTexture"/>
            <children>
                <componentref id="treetop"/>
                <componentref id="treetrunk"/>
            </children>
        </component>
        <component id = "tree_2">
            <transformation>
                <translate x = "1" y = "0" z = "2" />
                <scale x="1.5" y="1" z="1" />
            </transformation>
             <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="demoTexture"/>
            <children>
                <componentref id="treetop"/>
                <componentref id="treetrunk"/>
            </children>
        </component>
        <component id = "tree_3">
            <transformation>
                <translate x = "0" y = "0" z = "-2" />
                <scale x="1.5" y="2" z="1.5" />
            </transformation>
             <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="demoTexture"/>
            <children>
                <componentref id="treetop"/>
                <componentref id="treetrunk"/>
            </children>
        </component>
        <component id = "tree_4">
            <transformation>
                <translate x = "-2" y = "0" z = "2" />
            </transformation>
             <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="demoTexture"/>
            <children>
                <componentref id="treetop"/>
                <componentref id="treetrunk"/>
            </children>
        </component>
        <component id = "tree_5">
            <transformation>
                <translate x = "5" y = "0" z = "2" />
            </transformation>
             <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="demoTexture"/>
            <children>
                <componentref id="treetop"/>
                <componentref id="treetrunk"/>
            </children>
        </component>
        <component id = "tree_6">
            <transformation>
                <translate x = "-4" y = "0" z = "-4" />
            </transformation>
             <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="demoTexture"/>
            <children>
                <componentref id="treetop"/>
                <componentref id="treetrunk"/>
            </children>
        </component>

        <component id = "forest">
            <transformation>
            </transformation>
             <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="demoTexture"/>
            <children>
                <componentref id="tree_0"/>
                <componentref id="tree_1"/>
                <componentref id="tree_2"/>
                <componentref id="tree_3"/>
                <componentref id="tree_4"/>
                <componentref id="tree_5"/>
                <componentref id="tree_6"/>
            </children>
        </component>
        <component id = "back_floor">
            <transformation>
                <scale x = "40" y = "10" z = "20" />
                <rotate axis="x" angle="-90"/>
            </transformation>
             <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="demoTexture"/>
            <children>
                <primitiveref id = "rectangle" />
            </children>
        </component>
        
        <component id = "l_wall">
            <transformation>
                <translate x="0" y="1" z = "-1"/>
                <scale x = "2" y = "1" z = "1" />
                <rotate axis="y" angle="180"/>
            </transformation>
             <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="demoTexture"/>
            <children>
                <primitiveref id = "rectangle" />
            </children>
        </component>
        <component id = "r_wall">
            <transformation>
                <translate x="0" y="1" z = "1"/>
                <scale x = "2" y = "1" z = "1" />
            </transformation>
             <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="demoTexture"/>
            <children>
                <primitiveref id = "rectangle" />
            </children>
        </component>
        <component id = "f_wall">
            <transformation>
                <rotate axis="y" angle="90"/>
                <translate x="0" y="1" z = "-1"/>
                <scale x = "2" y = "1" z = "1" />
                <rotate axis="y" angle="180"/>
            </transformation>
             <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="demoTexture"/>
            <children>
                <primitiveref id = "rectangle" />
            </children>
        </component>
        <component id = "b_wall">
            <transformation>
                <rotate axis="y" angle="-90"/>
                <translate x="0" y="1" z = "-1"/>
                <scale x = "2" y = "1" z = "1" />
                <rotate axis="y" angle="180"/>
            </transformation>
             <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="demoTexture"/>
            <children>
                <primitiveref id = "rectangle" />
            </children>
        </component>

        <component id = "b_roof">
            <transformation>
               <translate x="0" y="2" z = "1"/>
                <rotate axis="x" angle="-45"/>
            </transformation>
            <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="demoTexture"/>
            <children>
                <primitiveref id = "triangle" />
            </children>
        </component>
        
        <component id = "l_roof">
            <transformation>
                <translate x="0" y="2" z = "-1"/>
                <rotate axis="y" angle="180"/>
                <rotate axis="x" angle="-45"/>
            </transformation>
             <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="demoTexture"/>
            <children>
                <primitiveref id = "triangle" />
            </children>
        </component>
        <component id = "r_roof">
            <transformation>
                <translate x="1" y="2" z = "0"/>
                <rotate axis="y" angle="90"/>
                <rotate axis="x" angle="-45"/>
            </transformation>
             <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="demoTexture"/>
            <children>
                <primitiveref id = "triangle" />
            </children>
        </component>
         <component id = "f_roof">
            <transformation>
                <translate x="-1" y="2" z = "0"/>
                <rotate axis="y" angle="-90"/>
                <rotate axis="x" angle="-45"/>
            </transformation>
             <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="demoTexture"/>
            <children>
                <primitiveref id = "triangle" />
            </children>
        </component>
        <component id = "enfeite">
            <transformation>
                <translate x="-1" y="2" z = "0"/>
                <rotate axis="y" angle="-90"/>
                <scale x = "0.1" y = "0.1" z = "0.1" />
            </transformation>
             <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="demoTexture"/>
            <children>
                <primitiveref id = "torus" />
            </children>
        </component>


        <component id = "house">
            <transformation>
            </transformation>
             <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="demoTexture"/>
            <children>
                <componentref id = "l_wall" />
                <componentref id = "r_wall" />
                <componentref id = "f_wall" />
                <componentref id = "b_wall" />
                <componentref id = "b_roof" />
                <componentref id = "l_roof" />
                <componentref id = "r_roof" />
                <componentref id = "f_roof" />
                <componentref id = "enfeite" />
            </children>
        </component>




        <component id = "sculp_0">
            <transformation>
                <scale x = "0.4" y = "0.4" z = "0.4" />
                <translate x="2" y="1" z = "2"/>
            </transformation>
             <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="demoTexture"/>
            <children>
                <primitiveref id = "sphere" />
            </children>
        </component>
        <component id = "sculp_1">
            <transformation>
                <scale x = "0.3" y = "0.3" z = "0.3" />
                <translate x="2.72" y="3.5" z = "2.72"/>
            </transformation>
             <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="demoTexture"/>
            <children>
                <primitiveref id = "sphere" />
            </children>
        </component>
        <component id = "sculp_2">
            <transformation>
                <scale x = "0.2" y = "0.2" z = "0.2" />
                <translate x="4" y="7.5" z = "4"/>
            </transformation>
             <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="demoTexture"/>
            <children>
                <primitiveref id = "sphere" />
            </children>
        </component>
         <component id = "sculp">
            <transformation>
             <translate x="2" y="0" z = "2"/>
            </transformation>
             <materials>
                <material id="demoMaterial" />
            </materials>
            <texture id="demoTexture"/>
            <children>
                <componentref id = "sculp_0" />
                <componentref id = "sculp_1" />
                <componentref id = "sculp_2" />
            </children>
        </component>
    </components>
</lxs>